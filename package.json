{
  "name": "eslint-formatter-github",
  "version": "0.4.2",
  "description": "See ESLint errors and warnings directly in pull requests",
  "license": "MIT",
  "repository": "hipstersmoothie/eslint-formatter-github",
  "main": "dist/index.js",
  "author": {
    "name": "Andrew Lisowski",
    "email": "lisowski54@gmail.com"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "yarn build --watch",
    "lint": "eslint src --ext .ts,.js --format './dist/index.js'",
    "release": "auto shipit"
  },
  "keywords": [
    "eslint",
    "eslint-formatter",
    "formatter",
    "reporter",
    "lint",
    "validate",
    "github",
    "annotations",
    "checks"
  ],
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "eslint": ">= 5.x"
  },
  "dependencies": {
    "@octokit/rest": "^16.28.7",
    "create-check": "^0.5.0",
    "eslint-formatter-pretty": "^2.1.1"
  },
  "devDependencies": {
    "@types/env-ci": "3.1.0",
    "@types/eslint": "6.1.1",
    "@types/lru-cache": "5.1.0",
    "@typescript-eslint/eslint-plugin": "2.3.2",
    "@typescript-eslint/parser": "2.3.2",
    "auto": "7.6.2",
    "auto-config-hipstersmoothie": "1.0.4",
    "eslint": "6.5.1",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-config-xo": "0.27.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsdoc": "17.1.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "husky": "3.0.7",
    "lint-staged": "9.4.1",
    "prettier": "1.18.2",
    "strip-ansi": "5.2.0",
    "typescript": "3.6.3"
  },
  "ava": {
    "serial": true
  },
  "lint-staged": {
    "*.{js,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "auto": {
    "extends": "hipstersmoothie"
  }
}
